    <script>
    document.addEventListener('DOMContentLoaded', function() {
      const closeModalButton = document.querySelector("#detailModal .close");
      closeModalButton.addEventListener('click', function() {
        $('#detailModal').modal('hide');
      });
    });

    function showOptions(title, urls) {
      const modalContent = title.map((t, index) => {
        const url = urls[index];
        return `<p><a class="btn light btn-outline-secondary  mb-2" style="background-color: #001f3f; width: 200px;" href="${url}"><span class="badge light">${t}</span></a></p>`;
      }).join('');
      const modalContentElement = document.getElementById("modalContent");
      modalContentElement.innerHTML = modalContent;
      $('#detailModal').modal('show');
    }
  </script>
  
